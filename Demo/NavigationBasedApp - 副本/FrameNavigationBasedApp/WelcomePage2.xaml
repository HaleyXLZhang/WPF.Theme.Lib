      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FrameNavigationBasedApp"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="WelcomePage2">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <TextBlock>
            <Hyperlink NavigateUri="WelcomePage.xaml" >WelcomePage1</Hyperlink>
        </TextBlock>

        <TextBlock Grid.Row="1" Text="Welcome to WPF World1." VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Grid>
</Page>
